####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: nim.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) nim.pro
qmake: 
	@$(QMAKE) nim.pro

dist: 
	@mkdir -p .tmp/nim && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/nim/ && ( cd `dirname .tmp/nim` && $(TAR) nim.tar nim && $(GZIP) nim.tar ) && $(MOVE) `dirname .tmp/nim`/nim.tar.gz . && $(DEL_DIR) .tmp/nim

